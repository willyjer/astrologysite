{
  "name": "astro-anon",
  "version": "1.0.0",
  "description": "AI-powered astrology readings application",
  "private": true,
  "author": "AstroAnew Team",
  "keywords": [
    "astrology",
    "ai",
    "nextjs",
    "typescript",
    "react"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "prune": "ts-prune",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze": "ANALYZE=true npm run build",
